          <oc:var_label type="alphanumeric">Top (m)</oc:var_label>
          <oc:show_val>Opening elevation: 885.583 m.</oc:show_val>
        </arch:property>
        <arch:property>
          <arch:variableID>8CE688CD-F2B9-4308-AF1D-1765A03240CF</arch:variableID>
          <arch:valueID>B4EFDF48-C500-41BD-4192-F5D3584D2BF6</arch:valueID>

          <oc:show_val>IX</oc:show_val>
        </arch:property>
        <arch:property>
          <arch:variableID>0C351ACF-AD2B-4D9E-CCE4-AAA50DA8A986</arch:variableID>
          <arch:valueID>47D75997-99B2-4B38-8223-020D9BDFB4A6</arch:valueID>
